{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/aiidalab/aiidalab/master/aiidalab/registry/schemas/v2/app.schema.json",
    "$ref": "#/definitions/App",
    "definitions": {
        "App": {
            "type": "object",
            "properties": {
                "metadata": {
		    "$ref": "metadata.schema.json#/definitions/Welcome"
                },
                "name": {
                    "type": "string"
                },
                "releases": {
                    "type": "object",
                    "patternProperties": {
                        ".*": {
                            "$ref": "#/definitions/Release"
                        }
                    },
                    "propertyNames": {
                        "pattern": ".*"
                    }
                }
            },
            "additionalProperties": false,
            "required": [
                "metadata",
                "releases"
            ],
            "title": "App"
        },
        "Release": {
            "properties": {
                "metadata": {
		    "$ref": "metadata.schema.json#/definitions/Welcome"
                },
                "environment": {
		    "$ref": "environment.schema.json#/definitions/Environment"
                },
                "url": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "required": [
                "metadata",
                "environment"
            ]
        }
    }
}
